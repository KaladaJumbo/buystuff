<h1 class=title><%= render partial: "shared/homeicon"%>Item Index</h1>
<div class = container id = items_con>
    <h3 id = items_title>Items</h3>
    <div class = "container">
        <ul class =list-inline id = items_list>
            <%@items.each do |item|%>
                <li><%=link_to(item.description, item_path(item))%></li>
            <%end%>
        </ul>
    <div>
</div>